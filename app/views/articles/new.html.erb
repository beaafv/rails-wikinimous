<h1>new article</h1>
<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>


<button type="button" class="btn btn-light text-dark">
<%= link_to 'back', articles_path %>
</button>
